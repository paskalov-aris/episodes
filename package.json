{
  "name": "episodes",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "packages:install": "yarn install",
    "packages:uninstall": "rm -rf node_modules/",
    "packages:reinstall": "yarn run packages:uninstall && yarn run packages:install",
    "gems:install": "bundle install",
    "gems:uninstall": "rm -rf vendor/ .bundle/",
    "gems:reinstall": "yarn run gems:uninstall && yarn run gems:install",
    "pods:install": "bundle exec pod install --project-directory=ios",
    "pods:uninstall": "rm -rf ios/Pods/",
    "pods:reinstall": "yarn run pods:uninstall && yarn run pods:install",
    "deps:install": "yarn run packages:install && yarn run pods:install",
    "deps:uninstall": "yarn run packages:uninstall && yarn run pods:uninstall",
    "deps:reinstall": "yarn run deps:uninstall && yarn run deps:install"
  },
  "dependencies": {
    "@react-native-firebase/app": "^19.1.2",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "date-fns": "^3.6.0",
    "expo": "^50.0.0",
    "expo-av": "~13.10.5",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.30.1",
    "react-native-svg": "^15.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.6.3",
    "prettier": "^3.2.5",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
